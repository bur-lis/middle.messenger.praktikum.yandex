{
    "compilerOptions": {
        "target": "ES2020",
        "types": [
            "node"
          ],
        "moduleResolution": "node",
        "allowImportingTsExtensions": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,

        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "skipLibCheck": true
    },
    "include": [
        "src//*",
        "src/**/*",
        "package.json"
, "hendlbars_precompille.ts"    ],
}